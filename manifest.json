{
  "name": "WikiNediyo",
  "description" : "What was wikipedia said about the text you selected...",
  "version": "2019.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "WikiNediyo",
    "default_icon": "./img/wikinediyo_16.png"
  },
  "icons": {
    "16": "./img/wikinediyo_16.png",
    "32": "./img/wikinediyo_32.png",
    "64": "./img/wikinediyo_64.png",
    "128": "./img/wikinediyo_128.png",
    "256": "./img/wikinediyo_256.png",
    "512": "./img/wikinediyo_512.png"
  },
  "background" : {
    "scripts" : ["./js/eventPage.js"],
    "persistent": false
  },
  "permissions": ["tabs","contextMenus"]
}
